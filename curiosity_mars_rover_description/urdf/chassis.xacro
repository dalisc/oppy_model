	</link>

  	<joint name="right_axis_joint" type="fixed">
		<parent link="chassis"/>
		<child link="right_axis"/>
		<origin xyz="-0.7938 -0.2133 0" rpy="0 0.19565141 0"/>
	</joint>


	<link name='suspension_arm_F_R'>
